<Window x:Class="CheckConnectionWpf.Views.CompareConnectionsForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CheckConnectionWpf.Views"
        mc:Ignorable="d"
        Title="Сравнение подключений" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
            <Label Grid.Row="0" Grid.Column="0" Content="Текущее соединение"/>
            <Label Grid.Row="0" Grid.Column="1"/>
            <ListBox x:Name="listBoxActiveConnection" Grid.Row="1" Grid.Column="0"/>
            <ListBox x:Name="listBoxHistoryConnection" Grid.Row="1" Grid.Column="1"/>
    </Grid>
</Window>
